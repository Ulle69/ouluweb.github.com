        {% for post in site.categories.podcast limit:10 %}

        <item>

            <title>{{ post.title }}</title>

            <itunes:author>Tommi Pääkkö &amp; Olli Aapro</itunes:author>

            <itunes:subtitle>Frontend Friday: {{ post.title }}</itunes:subtitle>

            <itunes:summary>{{ post.content | xml_escape }}</itunes:summary>

            <itunes:image href="http://ouluweb.github.com/assets/itunes.jpg" />

            <enclosure url="{{ post.link }}" length="{{ post.length }}" type="audio/x-m4a" />

            <guid>{{ post.link }}</guid>

            <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S" }} GMT</pubDate>

            <itunes:duration>{{ post.duration }}</itunes:duration>

            <itunes:keywords></itunes:keywords>

        </item>

        {% endfor %}

    </channel>

